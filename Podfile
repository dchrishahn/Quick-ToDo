# Uncomment the next line to define a global platform for your project
platform :ios, '11.2'

target 'Quick-ToDo' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Quick-ToDo
    pod 'Firebase/Auth'
    pod 'Firebase/Database'

end

target 'Quick-ToDoExtension' do
    # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
    use_frameworks!
    
    # Pods for Quick-ToDoExtension
    pod 'Firebase/Auth'
    pod 'Firebase/Database'
    
end
